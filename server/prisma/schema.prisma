generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model user {
  id Int @id @default(autoincrement())
  first_Name String
  last_Name String
  Email     String @unique
  user_Name String @unique
  password String

  @@map("user_tbl")
}
